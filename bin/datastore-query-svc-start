#!/bin/sh

# start datastore-service application (query api service)
cd ~/datastore-support/repos/datastore-service
mvn spring-boot:run -Dspring-boot.run.arguments="--datastore.server.repository.location=$DATASTORE_SERVER_REPOSITORY_LOCATION --datastore.service.influxdb.token=$DATASTORE_SERVICE_INFLUXDB_TOKEN --spring.data.mongodb.username=$SPRING_DATA_MONGODB_USERNAME --spring.data.mongodb.password=$SPRING_DATA_MONGODB_PASSWORD" & echo $! > /tmp/datastore-service.pid &

